# Simple configuration file
# Copy this to .env and adjust if needed

# Change this to something unique for security
WEBUI_SECRET_KEY=changethissecretkey123456

# Name shown in the interface
WEBUI_NAME=Local AI Chat

# Allow new users to sign up (set to false after creating your account)
ENABLE_SIGNUP=true

# Port for the web interface (change if 3000 is already used)
WEB_PORT=3000

# Port for Ollama API (change if 11434 is already used)  
OLLAMA_PORT=11434